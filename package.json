{
  "name": "json-casefy",
  "version": "1.0.0",
  "description": "Transform JSON object keys between different case styles using DFS algorithm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pnpm run build",
    "pretest": "eslint . --fix && prettier --write ./src/**/*.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && prettier \"{src,apps,libs,test}/**/*.ts\" --write",
    "prepush": "pnpm run test",
    "precommit": "lint-staged",
    "prepare": "husky",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "release": "release-please release-pr --token=$GITHUB_TOKEN --repo-url=https://github.com/DMBerlin/json-casefy.git --config-file=package.json",
    "release:create": "release-please release-pr --token=$GITHUB_TOKEN --repo-url=https://github.com/DMBerlin/json-casefy.git --config-file=package.json",
    "release:tag": "release-please release-pr --token=$GITHUB_TOKEN --repo-url=https://github.com/DMBerlin/json-casefy.git --config-file=package.json --create-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DMBerlin/json-casefy.git"
  },
  "keywords": [
    "json",
    "case",
    "conversion",
    "camelCase",
    "snake_case",
    "kebab-case",
    "PascalCase",
    "transform",
    "typescript",
    "npm-package"
  ],
  "author": "Daniel Marinho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DMBerlin/json-casefy/issues"
  },
  "homepage": "https://github.com/DMBerlin/json-casefy#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "release-please": "^17.1.3",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "/__mocks__/",
      "/__tests__/",
      "/interfaces/",
      "/types/",
      "/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@fixtures/(.*)$": "<rootDir>/src/__fixtures__/$1",
      "^@common/(.*)$": "<rootDir>/src/common/$1",
      "^@core/(.*)$": "<rootDir>/src/core/$1",
      "^@helpers/(.*)$": "<rootDir>/src/helpers/$1",
      "^@interfaces/(.*)$": "<rootDir>/src/interfaces/$1",
      "^@transformers/(.*)$": "<rootDir>/src/transformers/$1",
      "^@lib-types/(.*)$": "<rootDir>/src/types/$1"
    }
  }
}
